        </StackPanel>

        <Grid  Grid.Row="1">
            <WrapPanel Orientation="Vertical">
                <Label Content="侧入式雨水斗"></Label>
            </WrapPanel>
            <cus:ImageButton Content="->"  Tag="侧入式雨水斗" HorizontalAlignment="Right" ImageTextLocation="TextOnly" Name="btnSet" Click="btnSet_Click"  Margin="5,0,0,0" Width="30"  TabIndex="3"></cus:ImageButton>
        </Grid>

        <Grid  Grid.Row="2">
            <WrapPanel Orientation="Vertical">
